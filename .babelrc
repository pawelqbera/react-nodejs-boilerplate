{
  "presets": [
    "react",
    "es2015",
    "stage-0"
  ],
  "plugins": [
    [
      "transform-decorators-legacy",
      "syntax-dynamic-import"
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-react-remove-prop-types"
      ]
    },
    "development": {
      "plugins": [
        "transform-react-jsx-source"
      ]
    },
    "test": {
      "plugins": [
        "dynamic-import-node"
      ]
    },
    "development-hot": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}
